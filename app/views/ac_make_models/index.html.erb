<p id="notice"><%= notice %></p>

<h1>Ac Make Models</h1>

<table>
  <thead>
    <tr>
      <th>Abbreviation</th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Family</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ac_make_models.each do |ac_make_model| %>
      <tr>
        <td><%= ac_make_model.abbreviation %></td>
        <td><%= ac_make_model.manufacturer %></td>
        <td><%= ac_make_model.model %></td>
        <td><%= ac_make_model.family %></td>
        <td><%= link_to 'Show', ac_make_model %></td>
        <td><%= link_to 'Edit', edit_ac_make_model_path(ac_make_model) %></td>
        <td><%= link_to 'Destroy', ac_make_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ac Make Model', new_ac_make_model_path %>

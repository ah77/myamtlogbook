<p id="notice"><%= notice %></p>

<h1>Technicians</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Email</th>
      <th>Certificate number</th>
      <th>Certificate type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @technicians.each do |technician| %>
      <tr>
        <td><%= technician.name %></td>
        <td><%= technician.username %></td>
        <td><%= technician.email %></td>
        <td><%= technician.certificate_number %></td>
        <td><%= technician.certificate_type %></td>
        <td><%= link_to 'Show', technician %></td>
        <td><%= link_to 'Edit', edit_technician_path(technician) %></td>
        <td><%= link_to 'Destroy', technician, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Technician', new_technician_path %>
